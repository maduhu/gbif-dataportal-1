<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans SYSTEM "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!-- Property Store-->
	<bean id="servicePropertyStore" class="org.gbif.portal.util.propertystore.impl.spring.PropertyStoreImpl">
		<property name="namespaceMappings">
			<map>
				<entry key="http://gbif.org/portal-service/2006/1.0" value-ref="propertyStore:namespace:GBIF:Portal-Service:1.0"/>						
				<entry key="http://gbif.org/portal-service/occurrenceOutput/2006/1.0" value-ref="propertyStore:namespace:GBIF:Portal-Service:OccurrenceOutput:1.0"/>										
				<entry key="http://gbif.org/portal-service/taxonomyOutput/2006/1.0" value-ref="propertyStore:namespace:GBIF:Portal-Service:TaxonomyOutput:1.0"/>														
			</map>
		</property>
	</bean>
</beans>