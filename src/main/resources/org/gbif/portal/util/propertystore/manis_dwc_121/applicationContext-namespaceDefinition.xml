						<property name="XPath">
							<bean parent="abstractXPath:MaNIS:DWC:1.21">
								<constructor-arg value="//dwc:YearCollected"/>
							</bean>
						</property>
					</bean>
				</entry>			
				<entry key="RECORD.TEMPORAL.MONTH">
