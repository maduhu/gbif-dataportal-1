						</property>
					</bean>
				</entry>			
				<entry key="RECORD.TAXONOMY.GENUS">
					<bean class="org.gbif.portal.util.mhf.message.impl.xml.accessor.StringFromXPathAccessor">
						<property name="XPath">
							<bean parent="abstractXPath:MaNIS:DWC:1.21">
