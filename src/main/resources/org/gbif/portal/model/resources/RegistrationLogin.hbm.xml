<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class table="registration_login" name="org.gbif.portal.model.resources.RegistrationLogin">
    <id unsaved-value="null" name="id" column="id">
      <generator class="native"/>
    </id>
    <property name="loginId" column="login_id" not-null="true"/>
    <property name="businessKey" column="business_key" not-null="true"/>
  </class>
</hibernate-mapping>