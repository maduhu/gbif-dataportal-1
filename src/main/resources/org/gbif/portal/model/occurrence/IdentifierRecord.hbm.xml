<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="identifier_record" name="org.gbif.portal.model.occurrence.IdentifierRecord">
    <id unsaved-value="null" name="id" column="id">
      <generator class="native"/>
    </id>
    <property name="dataResourceId" column="data_resource_id" not-null="true" insert="false" update="false"/>
    <property name="occurrenceRecordId" column="occurrence_id" not-null="true" insert="false" update="false"/>
    <property name="identifierType" column="identifier_type" not-null="true" type="org.gbif.portal.model.occurrence.IdentifierType"/>
    <property name="identifier" column="identifier" not-null="true" insert="false" update="false"/>
  </class>
</hibernate-mapping>
