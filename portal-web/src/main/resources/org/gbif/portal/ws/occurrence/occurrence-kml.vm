  <name>$folder.getTaxonName()</name>
#foreach ($placemarkMap in $results.get($folder))
  <Placemark>
   <name><![CDATA[$placemarkMap.get("name")]]></name>
   <description><![CDATA[$placemarkMap.get("description")]]></description>
   <styleUrl>$placemarkMap.get("styleUrl")</styleUrl>
   <Point>
    <coordinates>$placemarkMap.get("coordinates")</coordinates>
